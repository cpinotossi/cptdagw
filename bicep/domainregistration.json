            "type": "SecureString"
        },
        "domains_cptdagw_org_nameFirst": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameLast": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_phone": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_email_1": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameFirst_1": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameLast_1": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_phone_1": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_email_2": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameFirst_2": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameLast_2": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_phone_2": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_email_3": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameFirst_3": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_nameLast_3": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_phone_3": {
            "type": "SecureString"
        },
        "domains_cptdagw_org_name": {
            "defaultValue": "cptdagw.org",
            "type": "String"
        },
        "dnszones_cptdagw_org_externalid": {
            "defaultValue": "/subscriptions/f474dec9-5bab-47a3-b4d3-e641dac87ddb/resourceGroups/cptdagw/providers/Microsoft.Network/dnszones/cptdagw.org",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DomainRegistration/domains",
            "apiVersion": "2022-09-01",
            "name": "[parameters('domains_cptdagw_org_name')]",
            "location": "global",
            "properties": {
                "privacy": true,
                "autoRenew": false,
                "dnsType": "AzureDns",
                "dnsZoneId": "[parameters('dnszones_cptdagw_org_externalid')]",
                "consent": {},
                "contactAdmin": {
                    "email": "[parameters('domains_cptdagw_org_email')]",
                    "nameFirst": "[parameters('domains_cptdagw_org_nameFirst')]",
                    "nameLast": "[parameters('domains_cptdagw_org_nameLast')]",
                    "phone": "[parameters('domains_cptdagw_org_phone')]"
                },
                "contactBilling": {
                    "email": "[parameters('domains_cptdagw_org_email_1')]",
                    "nameFirst": "[parameters('domains_cptdagw_org_nameFirst_1')]",
                    "nameLast": "[parameters('domains_cptdagw_org_nameLast_1')]",
                    "phone": "[parameters('domains_cptdagw_org_phone_1')]"
                },
                "contactRegistrant": {
                    "email": "[parameters('domains_cptdagw_org_email_2')]",
                    "nameFirst": "[parameters('domains_cptdagw_org_nameFirst_2')]",
                    "nameLast": "[parameters('domains_cptdagw_org_nameLast_2')]",
                    "phone": "[parameters('domains_cptdagw_org_phone_2')]"
                },
                "contactTech": {
                    "email": "[parameters('domains_cptdagw_org_email_3')]",
                    "nameFirst": "[parameters('domains_cptdagw_org_nameFirst_3')]",
                    "nameLast": "[parameters('domains_cptdagw_org_nameLast_3')]",
                    "phone": "[parameters('domains_cptdagw_org_phone_3')]"
                }
            }
        }
    ]
